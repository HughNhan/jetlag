# Create inventory playbook will generate this for you much easier
[bastion]
example.com ansible_ssh_user=root

[master]
# Network mac is the intended mac address of the private network to use for this cluster
# Lab mac corresponds to the last interface in ocpinventory which should be the lab dhcp network
# We disable the lab network for this type of cluster
master-0 bmc_address=mgmt-master-0.example.com mac=40:a6:b7:2a:74:40 lab_mac=bc:97:e1:7a:d4:41 ip=198.18.10.5
master-1 bmc_address=mgmt-master-1.example.com mac=40:a6:b7:2a:98:90 lab_mac=bc:97:e1:78:c7:f1 ip=198.18.10.6
master-2 bmc_address=mgmt-master-2.example.com mac=40:a6:b7:2a:6b:f0 lab_mac=bc:97:e1:7a:ce:71 ip=198.18.10.7

[master:vars]
vendor=Dell
bmc_user=quads
bmc_password=password
interface=ens1f0
network_prefix=24
gateway=198.18.10.1
dns=8.8.8.8

[rwn]
rwn-0 bmc_address=mgmt-rwn-0.example.com mac=40:a6:b7:2a:75:f1 ip=198.18.16.2 gateway=198.18.16.1 vlan=100 interface=ens1f1.100
rwn-1 bmc_address=mgmt-rwn-1.example.com mac=40:a6:b7:2b:bc:01 ip=198.18.16.6 gateway=198.18.16.5 vlan=101 interface=ens1f1.101

[rwn:vars]
vendor=Dell
bmc_user=quads
bmc_password=password
network_prefix=30
network_mask=255.255.255.252
dns=8.8.8.8
